<Page x:Class="KPU_Faculty_Scheduler.SchedulePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KPU_Faculty_Scheduler"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="850"
      Title="SchedulePage" Width="850" Height="550">

    <Grid Width="850" Height="550">
        <!-- Rectangle to contain elements -->
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Margin="0,60,0,0" Stroke="Black" Width="850"/>

        <!-- Page Header & listbox -->
        <Label x:Name="roomHeader" Content="Schedule" HorizontalAlignment="Center" Margin="337,0,298,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="36" Width="165"/>

    </Grid>
</Page>
